{
    "name": "iftodi-petru/generate-pdf-tests",
    "description": "Plugin WordPress pentru generarea PDF-urilor cu teste",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.4",
        "yahnis-elsts/plugin-update-checker": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "GeneratePdfTests\\": "src/"
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "optimize-autoloader": true
    }
}
